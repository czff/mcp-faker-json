{
  "name": "mcp-faker-json",
  "version": "1.1.3",
  "description": "MCP server for mock faker json",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-faker-json": "dist/index.js"
  },
  "scripts": {
    "build": "tsdown && chmod 755 dist/index.js",
    "serve": "node dist/index.js & npx @modelcontextprotocol/inspector",
    "serve:sse": "node dist/index.js --transport=sse & npx @modelcontextprotocol/inspector",
    "serve:http": "node dist/index.js --transport=http & npx @modelcontextprotocol/inspector"
  },
  "keywords": [
    "mcp",
    "faker-json",
    "yapi",
    "mock"
  ],
  "author": {
    "name": "amie",
    "email": "2543415495@qq.com"
  },
  "copyright": "Copyright (c) 2025 amie. All rights reserved.",
  "license": "MIT",
  "pricing": "Free",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.10",
    "tsdown": "^0.12.9",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.0",
    "commander": "^14.0.0",
    "express": "^5.1.0",
    "zod": "^3.25.75"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=18.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/czff/mcp-faker-json"
  },
  "sideEffects": false
}
